{
  "name": "SnowMo",
  "version": "0.1.0",
  "scripts": {
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create NoahZinsmeister/SnowMo --node https://api.thegraph.com/deploy/",
    "create-local": "graph create NoahZinsmeister/SnowMo --node http://127.0.0.1:8020",
    "codegen": "graph codegen --debug --output-dir src/types/",
    "build": "graph build --debug",
    "deploy": "graph deploy NoahZinsmeister/SnowMo --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy --subgraph-name NoahZinsmeister/SnowMo --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.7.1",
    "@graphprotocol/graph-ts": "^0.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}